{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "yueling",
  "version": "0.1.0",
  "identifier": "com.shuiling.yueling",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "yueling",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "icons/icon.ico",
      "icons/icon.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/32x32.png",
      "icons/icon.icns"
    ],
    "android": {
      "signing": {
        "keystore": "android.keystore",
        "alias": "${ANDROID_KEY_ALIAS}",
        "password": "${ANDROID_KEY_PASSWORD}",
        "keystorePassword": "${ANDROID_KEYSTORE_PASSWORD}"
      }
    }
  }
}
